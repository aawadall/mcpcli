{
  "$schema": "https://schemas.modelcontextprotocol.org/server-config.json",
  "name": "{{.Config.Name}}",
  "version": "{{.Config.Version}}",
  "description": "{{.Config.Description}}",
  "author": "{{.Config.Author}}",
  "license": "MIT",
  "transport": {
    "type": "stdio",
    "options": {
      "command": "./{{.Config.Name}}"
    }
  },
  "capabilities": {
    "resources": {
      "enabled": true
    },
    "tools": {
      "enabled": true
    },
    "prompts": {
      "enabled": false
    }
  }
} 